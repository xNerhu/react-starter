{
  "name": "react-starter",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A React and TypeScript boilerplate",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nersent/react-starter.git"
  },
  "scripts": {
    "watch":
      "npm run build:dev && webpack-dev-server --hot --inline --port 8181 --color --progress",
    "build:dev": "webpack --progress --color",
    "build:production":
      "cross-env NODE_ENV=production webpack --progress --colors --define process.env.NODE_ENV='production'"
  },
  "devDependencies": {
    "@types/node": "9.4.6",
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "cross-env": "5.1.3",
    "file-loader": "1.1.6",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "ts-loader": "3.5.0",
    "tslint": "5.9.1",
    "tslint-config-airbnb": "5.6.0",
    "tslint-config-prettier": "1.8.0",
    "typescript": "2.7.2",
    "uglifyjs-webpack-plugin": "1.1.4",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  },
  "bugs": {
    "url": "https://github.com/nersent/react-starter/issues"
  },
  "homepage": "https://github.com/nersent/react-starter#readme",
  "keywords": ["react", "starter", "webpack", "react", "typescript"],
  "author": "Nersent"
}
