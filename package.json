{
  "name": "react-starter",
  "version": "0.0.5",
  "license": "MIT",
  "description": "React starter app for everyone.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nersent/react-starter.git"
  },
  "scripts": {
    "watch": "npm run build:dev && webpack-dev-server --hot --inline --port 8080 --colors --progress",
    "build:dev": "rimraf build && webpack --progress --colors",
    "build:production": "rimraf build && webpack --progress --colors --define process.env.NODE_ENV=production"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.7",
    "node-sass": "4.7.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "uglifyjs-webpack-plugin": "1.1.4",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  },
  "bugs": {
    "url": "https://github.com/nersent/react-starter/issues"
  },
  "homepage": "https://github.com/nersent/react-starter#readme",
  "keywords": [
    "react",
    "starter",
    "electron",
    "webpack"
  ],
  "author": "Nersent"
}
